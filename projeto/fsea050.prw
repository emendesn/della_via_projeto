User Function FSEA050()
Local   cFiltSX2   := ""
Local   bFiltSX2   := NIL
Private nUsado     := 0
Private cCadastro  := STR0001 //"Monitor de Arquivos"
Private cFilUA5    := ""
Private cFilUA2    := ""
Private aRotina    := {}
Private aCores     := {}
Private cPula      := Chr(13)+Chr(10)
Private oTimer     := NIL

dbSelectArea("UA8")
dbGoTop()
While !Eof()
	aAdd(aCores, { "UA5_STATUS == '"+UA8->UA8_STATUS+"'", AllTrim(UA8->UA8_COR), UA8->UA8_Desc })
	dbSkip()
End

aAdd( aRotina ,{STR0002 ,'AxPesqui'       ,0,1}) //"Pesquisar"
aAdd( aRotina ,{STR0003 ,'AxVisual'       ,0,2}) //"Visualizar"
aAdd( aRotina ,{STR0004 ,'u_UA5Legend()'  ,0,2}) //"Legenda"

dbSelectArea("UA5")
mBrowse( ,,,, "UA5",,,,"UA5_STATUS",, aCores ,,, 4,{|x| u_UA5Timer(@oTimer,x)})

Return NIL


