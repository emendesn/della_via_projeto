Project Function D31VldMan()
Local lRet := .T.

If ValType(INCLUI) == "L"
	// Verifica se existe outro registro com a mesma chave
	If !ExistChav("PAH", M->PAH_LOJADV + M->PAH_PESSOA) .And. INCLUI
		lRet := .F.
	EndIf
EndIf

Return(lRet)
