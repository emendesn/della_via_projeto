	Reclock("TMP",.T.)
	TMP->TMP_COD    := TRBFAT->D2_COD
	TMP->TMP_DOC    := TRBFAT->F2_DOC
	TMP->TMP_CLI    := TRBFAT->F2_CLIENTE
