Function A030CGC(cTipPes)

Local lRetorno := .T.

If cTipPes == "F" .And. !(Len(AllTrim(M->A1_CGC))==11)
	lRetorno := .F.
ElseIf cTipPes == "J" .And. !(Len(AllTrim(M->A1_CGC))==14)  
	Help(" ",1,"CGC")     
	lRetorno := .F.
EndIf

Return lRetorno
