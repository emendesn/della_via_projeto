If ExistBlock( "MA030BUT" ) 
	If Valtype( aUsrBut := Execblock( "MA030BUT", .f., .f., { 4 } ) ) == "A" 
		AEval( aUsrBut, { |x| AAdd( aButtons, x ) } ) 
	EndIF 
EndIf
