/*/
STATIC FUNCTION RetX3Box(cCampo)
LOCAL aRET:={},nCONT
LOCAL cCBox
LOCAL aSvArea := SX3->(GetArea())

SX3->(DbSetOrder(2))
SX3->(DbSeek(cCampo))
cCBox := SX3->(X3Cbox())
   
BEGIN SEQUENCE 
   While !Empty(cCBOX)
      nCONT:=AT(";",cCBOX)
      IF nCont == 0
         AADD(aRET,SubStr(AllTrim(cCBOX),3))
         Exit
      Else
         AADD(aRET,SubStr(AllTrim(SUBSTR(cCBOX,1,nCONT-1)),3))
      Endif 
      cCBOX:=SUBSTR(cCBOX,nCONT+1)
   Enddo
ENDSEQUENCE
   
RestArea(aSvArea)

RETURN aRET

