@ nLin, 001 PSAY ""
nLin := nLin + 6

If Getmv("MV_ESTADO")=="SP" // Somente para filiais de Sao Paulo
   @ nLin,034 Psay SM0->M0_ENDENT+SM0->M0_COMPENT
Endif
@ nLin,133 Psay cNumNFi + If(nFormul > 1," / "+AllTrim(Str(nForAtu,6)),"")
