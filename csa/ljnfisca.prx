   @ nLin,034 Psay SM0->M0_CIDENT
   @ nLin,060 Psay SM0->M0_ESTENT
Endif
nLin := nLin + 1
@ nLin,94 Psay "X"
nLin := nLin + 1
If Getmv("MV_ESTADO")=="SP" // Somente para filiais de Sao Paulo
   @ nLin,034 Psay SM0->M0_TEL
